{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Serverless",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "sls:start",
        "--inspect-brk=5858"
      ],
      "outFiles": [
        "${workspaceFolder}/.esbuild/**/*.js"
      ],
      "port": 5858,
      "sourceMaps": true,
      "timeout": 120000,
      "protocol": "inspector"
    }
  ]
}